<% content_for :head_title do %>Coupons<% end %>
<% content_for :top_nav do %><%= render :partial => 'shared/top_nav',:locals => {:param => "myracemenu"}%><% end %>
<% content_for :top_area do %><%= render :partial => 'director/shared/top_nav', :locals => {:step => "2"}%><% end %>
<% content_for :extra_head do %><%= calendar_date_select_includes "red"%><% end %>
<table width="900" border="0" cellspacing="0" cellpadding="0">
<tr>
<td width="6">&nbsp;</td>
<td width="774" align="center" valign="bottom" class="tableHEAD"><p>Coupons</p></td>
<td width="120" class="sidebarHeaderText">Featured Events </td>
</tr>
<tr>
<td>&nbsp;</td>
<td class="content_bg">
	<div id="director_table_holder">
		
		<table width="100%" cellspacing="0">
				<tr class="director_header_row">
					<td width="20px"></td>
					<td>Coupon Code</td>
					<td>Value</td>
					<td>Expiration</td>
					<td></td>
					<td width="5px"></td>
				</tr>
		<% @dir_event.coupons.each do | coupon | %>
			<tr id="coupon_<%=coupon.id%>" class="<%= cycle('director_row1', 'director_row2') %>">
				<%= render :partial => 'coupon_row',:locals => {:coupon => coupon}%>
			</tr>
		<% end %>
		<tr id="active_q_p" style="display:none;">
			
		</tr>
		<tr id="add_q_p" height="25px">
			<td></td>
			<td colspan="3">
				<%= link_to_function("Add a coupon", update_page{|page|
					page.replace_html "active_q_p", :partial => 'add_coupon', :locals => {:coupon => Coupon.new}
					page["active_q_p"].show
					page["add_q_p"].hide
				}) %>
			</td>
			<td align="right"></td>
			<td></td>
		</tr>
						
				
		</table>
		
	
</td>
<td width="120" rowspan="2" align="left" valign="top" id="ads"><%= render :partial => 'shared/ads'%></td>
<td style="padding-right:10px;"></td>
</tr>
<tr>
<td>&nbsp;</td>
<td id="tableBOTTOMshadow">&nbsp;</td>
</tr>
<%= render :partial => 'shared/footer'%>
</table>