<% content_for :head_title do %>Team <%= @team.name %>' members<% end %>
<% content_for :top_nav do %><%= render :partial => 'shared/top_nav',:locals => {:param => "myracemenu"}%><% end %>
<% content_for :top_area do %><%= render :partial => 'director/shared/top_nav', :locals => {:step => "3"}%><% end %>
<% content_for :extra_head do %>

	<%= javascript_include_tag "default-values" %>
<% end %>
<table width="900" border="0" cellspacing="0" cellpadding="0">
<tr>
<td width="6">&nbsp;</td>
<td width="774" align="center" valign="bottom" class="tableHEAD"><p>Team <%= @team.name %>' members</p></td>
<td width="120" class="sidebarHeaderText">Featured Events </td>
</tr>
<tr>
<td>&nbsp;</td>
<td class="content_bg">
	<div id="director_table_holder">
	<table width="100%" cellspacing="0">

		<tr class="director_header_row">
				<td width="20px"></td>
				<td>First Name</td>
        <td>Last Name</td>
        <td>Gender</td>
        <td>Reg. Date</td>
        <td>Approved?</td>
				<td align="right"></td>
				
			</tr>
		<tr class="director_row1">
			<td></td>
			<td colspan="7"></td>
		</tr>
		<% @team.team_entries.each do | te | %>
        <tr class="<%=cycle('director_row1', 'director_row2') %>">
            <td width="20px"></td>
            <td><%= te.event_registration.first_name %></td>
            <td><%= te.event_registration.last_name %></td>
            <td><%= te.event_registration.sex %></td>
			      <td><%= te.created_at.strftime('%m/%d/%Y') %></td>
            <td><%= te.approved %></td>
            <td align="right"><%=link_to cog_button_image, :controller => "registration", :action => "edit", :id => te.event_registration.id%></td>
            
        </tr>

		<% end %>
		<tr class="director_row1">
			<td></td>
			<td colspan="7"><%=  %></td>
		</tr>

	</table>
	</div>

</td>
<td width="120" rowspan="2" align="left" valign="top" id="ads"><%= render :partial => 'shared/ads'%></td>
<td style="padding-right:10px;"></td>
</tr>
<tr>
<td>&nbsp;</td>
<td id="tableBOTTOMshadow">&nbsp;</td>
</tr>
<%= render :partial => 'shared/footer'%>
</table>


